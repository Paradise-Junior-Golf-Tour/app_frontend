{
  "name": "gatsby_strapi",
  "version": "1.0.0",
  "private": true,
  "description": "Strapi + Gatsby",
  "author": "John Robertson",
  "keywords": [
    "gatsby",
    "strapi"
  ],
  "scripts": {
    "develop": "clear && gatsby develop",
    "start": "wait-on http://localhost:1337/admin && gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "clear && gatsby clean",
    "schema": "clear && curl -X POST http://localhost:8000/__refresh",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.4",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.10.9",
    "@mui/lab": "^5.0.0-alpha.104",
    "@mui/material": "^5.10.10",
    "@mui/x-date-pickers": "^5.0.5",
    "@netlify/plugin-gatsby": "^3.5.0",
    "axios": "^1.1.3",
    "gatsby": "^4.6.1",
    "gatsby-cli": "^4.24.0",
    "gatsby-plugin-gatsby-cloud": "^4.7.0",
    "gatsby-plugin-image": "^2.6.0",
    "gatsby-plugin-material-ui": "^4.0.2",
    "gatsby-plugin-sharp": "^4.6.0",
    "gatsby-plugin-use-query-params": "^1.0.1",
    "gatsby-source-strapi": "^2.0.0",
    "gatsby-theme-material-ui": "^5.3.0",
    "gatsby-transformer-remark": "^5.6.0",
    "gatsby-transformer-sharp": "^4.6.0",
    "moment": "^2.29.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-loading-skeleton": "^3.1.0",
    "react-slick": "^0.28.1",
    "slick-carousel": "^1.8.1",
    "use-query-params": "^2.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "wait-on": "5.2.1"
